{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/bot/cron",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/bot/watchdog",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/match-graph/run",
      "schedule": "0 5 * * *"
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}
